<%= link_to "Home", plays_home_path %>
<div class='image-block'>
  <h1 id="timer"></h1>
  <img src="" id="image" style="height: 100px">
  <br /><br />
  <button onclick="addPlays()">Click me</button>
</div>
<br /><br /><br /><br />

<table id="plays" data-images='<%= raw @images.to_json %>'>

<%= tag("div", id: "random-images-url",  data: @images) %>
  <tr>
    <th>Time count</th>
    <th>Image</th>
  </tr>
  <% @plays.each do |play| %>
    <tr>
      <td><%= play.time_count %></td>
      <td><img src="<%= play.image_url %>" style='height: 50px'></td>
    </tr>
  <% end %>
</table>
